<Calendar.Root value={value} onValueChange={setValue} className={styles.Root}>
  <Calendar.YearsList className={styles.YearsList}>
    {({ years }) =>
      years.map((year) => (
        <Calendar.YearsCell
          value={year}
          className={styles.YearsCell}
          key={year.toString()}
        />
      ))
    }
  </Calendar.YearsList>
</Calendar.Root>